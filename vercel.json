{
  "buildCommand": "pnpm build",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "framework": "vite",
  "outputDirectory": "dist",
  "env": {
    "VITE_APP_TITLE": "宝爸Steven | 洛杉矶商业地产专家",
    "VITE_APP_LOGO": "/logo.svg"
  },
  "headers": [
    {
      "source": "/public/assets/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
